    683                  case OS_OPT_PEND_ABORT_ALL:
    684                  case OS_OPT_PEND_ABORT_1   | OS_OPT_POST_NO_SCHED:
    685                  case OS_OPT_PEND_ABORT_ALL | OS_OPT_POST_NO_SCHED:
