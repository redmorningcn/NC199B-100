    667          #endif
    668          
    669          #if OS_CFG_CALLED_FROM_ISR_CHK_EN > 0u
    670              if (OSIntNestingCtr > (OS_NESTING_CTR)0u) {             /* Not allowed to Pend Abort from an ISR                  */
